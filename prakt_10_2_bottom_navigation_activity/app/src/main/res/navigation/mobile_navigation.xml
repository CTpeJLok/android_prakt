<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.prakt_10_2_bottom_navigation_activity.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_photo"
        android:name="com.example.prakt_10_2_bottom_navigation_activity.ui.dashboard.DashboardFragment"
        android:label="@string/title_photo"
        tools:layout="@layout/fragment_photo" />

    <fragment
        android:id="@+id/navigation_info"
        android:name="com.example.prakt_10_2_bottom_navigation_activity.ui.notifications.NotificationsFragment"
        android:label="@string/title_info"
        tools:layout="@layout/fragment_info" />
